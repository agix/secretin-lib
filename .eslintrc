{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": false,
    "mocha": true
  },
  "globals": {
    "crypto": true,
    "Secretin": true,
    "__karma__": true,
    "module": true,
    "process": true,
  },
  "rules": {
    "no-throw-literal": 0
  },
  "plugins": [
    "mocha",
    "chai"
  ]
}
